#: butia/butia.py:44
msgid "adjust LED intensity between 0 and 255"
msgstr "ajusta la intensidad del LED entre 0 y 255"

#: butia/butia.py:45
msgid ""
"returns the object gray level encountered him as a number between 0 and 1023"
msgstr ""
"devuelve el nivel de gris del objeto en frente al mismo como un número entre "
"0 y 1023"

#: butia/butia.py:46
msgid "returns 1 when the button is press and 0 otherwise"
msgstr "devuelve 1 cuando el botón está presionado, 0 en otro caso"

#: butia/butia.py:47
msgid "returns the ambient light level as a number between 0 and 1023"
msgstr "devuelve el nivel de la luz ambiente como un número entre 0 y 1023"

#: butia/butia.py:48
msgid "returns the ambient temperature as a number between 0 and 255"
msgstr "devuelve la temperatura como un numero entre 0 y 255"

#: butia/butia.py:49
msgid ""
"returns the distance from the object in front of the sensor as a number "
"between 0 and 255"
msgstr ""
"devuelve la distancia del objeto en frente del sensor como un número entre 0 "
"y 255"

#: butia/butia.py:50
msgid "returns 0 or 1 depending on the sensor inclination"
msgstr "devuelve 0 o 1 dependiendo de la inclinación del sensor"

#: butia/butia.py:51
msgid "returns 1 when the sensors detects a magnetic field, 0 otherwise"
msgstr "devuelve 1 cuando el sensor detecta un campo magnético. 0 en otro caso"

#: butia/butia.py:52
msgid "switches from 0 to 1, the frequency depends on the vibration"
msgstr "alterna entre 0 y 1, lña frecuencia depende de la vibración"

#: butia/butia.py:169
msgid "battery charge Butia"
msgstr "carga de batería Butiá"

#: butia/butia.py:171
msgid "returns the battery charge as a number between 0 and 255"
msgstr "devuelve la carga de la batería como un número entre 0 y 255"

#: butia/butia.py:177
msgid "speed Butia"
msgstr "velocidad Butiá"

#: butia/butia.py:180
msgid ""
"set the speed of the Butia motors as a value between 0 and 1023, passed by "
"an argument"
msgstr ""
"setea la velocidad de los motores del Butiá como un valor entre 0 y 1023,"
"pasado como un argumento"

#: butia/butia.py:188
msgid "move the Butia robot forward"
msgstr "mueve el robot Butiá adelante"

#: butia/butia.py:198
msgid "move the Butia robot forward a predefined distance"
msgstr "mueve el robot Butiá adelante una distancia predefinida"

#: butia/butia.py:207
msgid "move the Butia robot backward"
msgstr "mueve el robot Butiá atrás"

#: butia/butia.py:216
msgid "move the Butia robot backward a predefined distance"
msgstr "mueve el robot Butiá atrás una distancia predefinida"

#: butia/butia.py:224
msgid "turn the Butia robot at left"
msgstr "gira el robot Butiá a la izquierda"

#: butia/butia.py:232
msgid "turn the Butia robot at right"
msgstr "gira el robot Butiá a la derecha"

#: butia/butia.py:238
msgid "turn Butia"
msgstr "girar Butiá"

#: butia/butia.py:241
msgid "turn the Butia robot x degrees"
msgstr "gira el robot Butiá x grados"

#: butia/butia.py:249
msgid "stop the Butia robot"
msgstr "detener el robot Butiá"

#: butia/butia.py:255
msgid "display Butia"
msgstr "pantalla Butiá"

#: butia/butia.py:256
msgid "Hello World    Butia            "
msgstr "Hola Mundo    Butia             "

#: butia/butia.py:258
msgid "print text in Butia robot 32-character ASCII display"
msgstr "imprime texto en la pantalla de 32 caracteres ASCII del Butiá"
